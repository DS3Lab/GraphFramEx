{
  "cmd": "python exp_synthetic/main.py",
  "preload": "MKL_THREADING_LAYER=GNU",
  "ckpt_root": ".",
  "version": "1.0.0",
  "machine":{
    "gpus-per-task":1,
    "mem": 5000
  },
  "meta": {
    "group": "checkpoints",
    "name": "syn5",
    "dest-arg": "yes",
    "dest-name": "dest"
  },
  "params": {
    "gpu": true,
    "data_save_dir": "data",
    "num_test_nodes": 200,
    "dataset": "syn5",
    "sparsity": [0.7, 0.99, 0.997],
    "explainer_name": [
      "random",
      "distance",
      "pagerank",
      "sa_node",
      "ig_node",
      "occlusion",
      "gnnexplainer",
      "pgmexplainer",
      "subgraphx"
    ]
  }
}
