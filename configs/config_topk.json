{
  "cmd": "python code/main.py",
  "preload": "MKL_THREADING_LAYER=GNU",
  "ckpt_root": ".",
  "version": "1.0.0",
  "machine": {
    "time": 600,
    "gpus-per-task": 2,
    "mem": 5000
  },
  "meta": {
    "group": "checkpoints/node_classification",
    "name": "topk",
    "dest-arg": "yes",
    "dest-name": "dest"
  },
  "params": {
    "data_save_dir": "data",
    "num_test": 100,
    "dataset": ["syn1", "syn3", "syn4", "syn5", "syn6"],
    "topk": [10, 20, 40, 100, 500],
    "explainer_name": [
      "random",
      "distance",
      "pagerank",
      "sa_node",
      "ig_node",
      "occlusion",
      "gnnexplainer",
      "pgmexplainer",
      "subgraphx"
    ]
  }
}
