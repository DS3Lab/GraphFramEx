{
  "cmd": "python code/main.py",
  "preload": "MKL_THREADING_LAYER=GNU",
  "ckpt_root": ".",
  "version": "1.0.0",
  "machine": {
    "time": 600,
    "gpus-per-task": 2,
    "mem": 5000
  },
  "meta": {
    "group": "checkpoints/node_classification",
    "name": "gnnexplainer_size",
    "dest-arg": "yes",
    "dest-name": "dest"
  },
  "params": {
    "explain_graph": "False",
    "data_save_dir": "data",
    "num_test": 100,
    "dataset": "syn1",
    "explainer_name": {
      "gnnexplainer": { "edge_size": [0, 0.005, 0.05, 0.5, 1, 5, 10, 100] },
      "pagerank": { "edge_size": 0.005 }
    }
  }
}
