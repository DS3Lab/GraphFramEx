{
  "cmd": "python code/main.py",
  "preload": "MKL_THREADING_LAYER=GNU",
  "ckpt_root": ".",
  "version": "1.0.0",
  "machine": {
    "time": 600,
    "gpus-per-task": 2,
    "mem": 5000
  },
  "meta": {
    "group": "checkpoints/graph_classification",
    "name": "sparsity_mask",
    "dest-arg": "yes",
    "dest-name": "dest"
  },
  "params": {
    "data_save_dir": "data",
    "num_test": 100,
    "hard_mask": ["True", "False"],
    "explain_graph": "True",
    "dataset": "mutag",
    "sparsity": [0, 0.7, 0.99, 0.997],
    "explainer_name": [
      "random",
      "sa",
      "ig",
      "occlusion",
      "gnnexplainer",
      "pgmexplainer",
      "subgraphx"
    ]
  }
}
